	file = {Halsbenning - 2021 - Digitalisierung öffentlicher Dienstleistungen Her.pdf:C\:\\Users\\s0572\\Zotero\\storage\\XQ4T5GKL\\Halsbenning - 2021 - Digitalisierung öffentlicher Dienstleistungen Her.pdf:application/pdf},
