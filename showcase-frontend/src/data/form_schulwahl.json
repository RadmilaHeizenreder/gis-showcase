{
  "components": [
    {
      "label": "Address",
      "tableView": false,
      "provider": "nominatim",
      "key": "address1",
      "type": "address",
      "providerOptions": {
        "params": {
          "autocompleteOptions": {}
        }
      },
      "validate": {
        "required": true
      },
      "input": true,
      "components": [
        {
          "label": "Address 1",
          "tableView": false,
          "key": "address1",
          "type": "textfield",
          "input": true,
          "customConditional": "show = _.get(instance, 'parent.manualMode', false);"
        },
        {
          "label": "Address 2",
          "tableView": false,
          "key": "address2",
          "type": "textfield",
          "input": true,
          "customConditional": "show = _.get(instance, 'parent.manualMode', false);"
        },
        {
          "label": "City",
          "tableView": false,
          "key": "city",
          "type": "textfield",
          "input": true,
          "customConditional": "show = _.get(instance, 'parent.manualMode', false);"
        },
        {
          "label": "State",
          "tableView": false,
          "key": "state",
          "type": "textfield",
          "input": true,
          "customConditional": "show = _.get(instance, 'parent.manualMode', false);"
        },
        {
          "label": "Country",
          "tableView": false,
          "key": "country",
          "type": "textfield",
          "input": true,
          "customConditional": "show = _.get(instance, 'parent.manualMode', false);"
        },
        {
          "label": "Zip Code",
          "tableView": false,
          "key": "zip",
          "type": "textfield",
          "input": true,
          "customConditional": "show = _.get(instance, 'parent.manualMode', false);"
        }
      ]
    },
    {

      "key": "myMap",
      "type": "customMap",
      "tooltip": "Bitte w√§hlen Sie eine Schule aus"
    },
    {
      "label": "1. Wahl Schule",
      "applyMaskOn": "change",
      "tableView": true,
      "key": "choice1",
      "type": "textfield",
      "input": true
    },
    {
      "label": "schoolId1",
      "key": "schoolId1",
      "type": "hidden",
      "input": true,
      "tableView": false
    },
    {
      "label": "prio1",
      "defaultValue": "1",
      "key": "prio1",
      "type": "hidden",
      "input": true,
      "tableView": false
    },
    {
      "label": "route1",
      "redrawOn": "choice1",
      "key": "route1",
      "type": "hidden",
      "input": true,
      "tableView": false
    },
    {
      "label": "2. Wahl Schule",
      "applyMaskOn": "change",
      "tableView": true,
      "key": "choice2",
      "type": "textfield",
      "input": true
    },
    {
      "label": "schoolId2",
      "key": "schoolId2",
      "type": "hidden",
      "input": true,
      "tableView": false
    },
    {
      "label": "prio2",
      "defaultValue": "2",
      "key": "prio2",
      "type": "hidden",
      "input": true,
      "tableView": false
    },
    {
      "label": "route2",
      "redrawOn": "choice2",
      "key": "route2",
      "type": "hidden",
      "input": true,
      "tableView": false
    },
    {
        "type": "button",
        "label": "Submit",
        "key": "submit",
        "disableOnInvalid": true,
        "input": true,
        "tableView": false
    }
  ]
